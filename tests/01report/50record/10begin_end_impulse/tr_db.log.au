CREATE_STREAM @5 {NAME:urm0 T:uvm_report_message stream SCOPE:UVM STREAM:1}
  BEGIN @5 {TXH:2 STREAM:1 NAME:uvm_trace_message TIME=5  TYPE="uvm_trace_message" LABEL:"TEST_BEGIN" DESC="Beginning..."}
    SET_ATTR @5 {TXH:2 NAME:filename VALUE:test.sv   RADIX:UVM_STRING BITS=56}
    SET_ATTR @5 {TXH:2 NAME:line VALUE:67   RADIX:UVM_UNSIGNED BITS=32}
    SET_ATTR @5 {TXH:2 NAME:severity VALUE:UVM_INFO   RADIX:UVM_STRING BITS=64}
    SET_ATTR @5 {TXH:2 NAME:verbosity VALUE:UVM_LOW   RADIX:UVM_STRING BITS=56}
    SET_ATTR @5 {TXH:2 NAME:id VALUE:TEST_BEGIN   RADIX:UVM_STRING BITS=80}
    SET_ATTR @5 {TXH:2 NAME:begin_message VALUE:Beginning...   RADIX:UVM_STRING BITS=96}
    SET_ATTR @5 {TXH:2 NAME:end_message VALUE:Ending...   RADIX:UVM_STRING BITS=72}
    SET_ATTR @5 {TXH:2 NAME:color VALUE:red   RADIX:UVM_STRING BITS=24}
    SET_ATTR @5 {TXH:2 NAME:my_int VALUE:5   RADIX:UVM_DEC BITS=32}
    SET_ATTR @5 {TXH:2 NAME:my_string VALUE:foo   RADIX:UVM_STRING BITS=24}
    SET_ATTR @5 {TXH:2 NAME:my_obj.foo VALUE:3   RADIX:UVM_DEC BITS=32}
    SET_ATTR @5 {TXH:2 NAME:my_obj.bar VALUE:hi there   RADIX:UVM_STRING BITS=64}
  END @5 {TXH:2 TIME=5}
CREATE_STREAM @10 {NAME:urm1 T:uvm_report_message stream SCOPE:UVM STREAM:3}
  BEGIN @10 {TXH:4 STREAM:3 NAME:uvm_trace_message TIME=10  TYPE="uvm_trace_message" LABEL:"TEST_BEGIN" DESC="Beginning..."}
    SET_ATTR @10 {TXH:4 NAME:filename VALUE:test.sv   RADIX:UVM_STRING BITS=56}
    SET_ATTR @10 {TXH:4 NAME:line VALUE:81   RADIX:UVM_UNSIGNED BITS=32}
    SET_ATTR @10 {TXH:4 NAME:severity VALUE:UVM_INFO   RADIX:UVM_STRING BITS=64}
    SET_ATTR @10 {TXH:4 NAME:verbosity VALUE:UVM_LOW   RADIX:UVM_STRING BITS=56}
    SET_ATTR @10 {TXH:4 NAME:id VALUE:TEST_BEGIN   RADIX:UVM_STRING BITS=80}
    SET_ATTR @10 {TXH:4 NAME:begin_message VALUE:Beginning...   RADIX:UVM_STRING BITS=96}
    SET_ATTR @10 {TXH:4 NAME:end_message VALUE:Ending...   RADIX:UVM_STRING BITS=72}
    SET_ATTR @10 {TXH:4 NAME:color VALUE:white   RADIX:UVM_STRING BITS=40}
    SET_ATTR @10 {TXH:4 NAME:my_string VALUE:hey buddy   RADIX:UVM_STRING BITS=72}
    SET_ATTR @10 {TXH:4 NAME:my_obj.foo VALUE:7   RADIX:UVM_DEC BITS=32}
    SET_ATTR @10 {TXH:4 NAME:my_obj.bar VALUE:bar   RADIX:UVM_STRING BITS=24}
  END @10 {TXH:4 TIME=10}
